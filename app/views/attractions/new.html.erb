  <%= simple_form_for @attraction do |f| %>
        <%= f.input :image, label: false%>
        <%= f.input :image_cache, as: :hidden %>
        <%= f.input :subject%>
        <%= f.input :content%>
        <%= f.input :address, :input_html => {id: "location-input"}%>
        <%= f.input :link%>
        <%= f.button :submit %>
      <% end %>


